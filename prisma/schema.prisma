// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:db.sqlite"
}

model Invoice {
  printNum   String   @id
  invoiceNum String
  date       DateTime
  orderNum   String?
}

model wmsorder {
  orderNum        String    @id
  date            DateTime
  acceptedWMSDate DateTime
  searchTSDate    DateTime?
}
